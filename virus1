def log_menu_s():
	s = raw_input("\033[0;91m>\033[0;97m>\033[0;94m> \033[0;96m")
	if s =="":
		print"\033[0;97m[\033[0;91m!\033[0;97m] Isi Yg Benar Bro !"
		log_menu_s()
	elif s =="1" or s =="01":
		log_cookie()
	elif s =="2"or s =="02":
		log_tokenz()
	elif s =="3"or s =="03":
		log_manual()
	elif s =="4"or s =="04":
		view_token()
	elif s =="0" or s =="00":
		back()
	else:
		print"\033[0;97m[\033[0;91m!\033[0;97m] Isi Yg Benar Bro !"
		log_menu_s()
		
